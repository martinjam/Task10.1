<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewFood">

    <TextView
        android:id="@+id/locationTextView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="37dp"
        android:text="Location:"
        app:layout_constraintStart_toEndOf="@+id/locationTextView3"
        app:layout_constraintTop_toBottomOf="@+id/quantityTextView2" />

    <TextView
        android:id="@+id/locationTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="42dp"
        android:text="Location:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/quantityTextView" />

    <Button
        android:id="@+id/saveFoodButton"
        android:layout_width="211dp"
        android:layout_height="45dp"
        android:layout_marginStart="87dp"
        android:layout_marginTop="55dp"
        android:text="ADD TO CART"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/priceFoodDisplayTextView" />

    <TextView
        android:id="@+id/descriptionTextView2"
        android:layout_width="338dp"
        android:layout_height="57dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="8dp"
        android:text="Description:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/descriptionTextView" />

    <TextView
        android:id="@+id/pickupTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="40dp"
        android:text="Pick up times:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dateTextView2" />

    <TextView
        android:id="@+id/pickupTextView2"
        android:layout_width="224dp"
        android:layout_height="21dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="40dp"
        android:text="Pick up times:"
        app:layout_constraintStart_toEndOf="@+id/pickupTextView"
        app:layout_constraintTop_toBottomOf="@+id/dateTextView2" />

    <TextView
        android:id="@+id/quantityTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="44dp"
        android:text="Quantity:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pickupTextView" />

    <TextView
        android:id="@+id/quantityTextView2"
        android:layout_width="186dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:text="Quantity:"
        app:layout_constraintStart_toEndOf="@+id/quantityTextView"
        app:layout_constraintTop_toBottomOf="@+id/pickupTextView2" />

    <TextView
        android:id="@+id/dateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="25dp"
        android:text="Date:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/descriptionTextView2" />

    <TextView
        android:id="@+id/dateTextView2"
        android:layout_width="293dp"
        android:layout_height="24dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="25dp"
        android:text="Date:"
        app:layout_constraintStart_toEndOf="@+id/dateTextView"
        app:layout_constraintTop_toBottomOf="@+id/descriptionTextView2" />

    <TextView
        android:id="@+id/priceFoodDisplayTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginTop="37dp"
        android:text="Price"
        app:layout_constraintStart_toEndOf="@+id/priceFoodTextView"
        app:layout_constraintTop_toBottomOf="@+id/locationTextView4" />

    <TextView
        android:id="@+id/priceFoodTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="37dp"
        android:text="Price:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/locationTextView3" />

    <TextView
        android:id="@+id/descriptionTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="8dp"
        android:text="Description:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="232dp"
        android:layout_height="136dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_camera" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="292dp"
        android:layout_height="49dp"
        android:layout_marginStart="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/saveFoodButton"
        app:srcCompat="@drawable/paypic" />

</androidx.constraintlayout.widget.ConstraintLayout>

